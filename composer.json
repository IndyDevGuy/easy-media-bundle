    "homepage": "https://github.com/agence-adeliom/easy-media-bundle",
    "support": {
        "issues": "https://github.com/agence-adeliom/easy-media-bundle/issues",
        "source": "https://github.com/agence-adeliom/easy-media-bundle"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Arnaud RITTI",
            "homepage": "https://github.com/arnaud-ritti"
        }
    ],
    "require": {
