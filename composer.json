        "league/flysystem": "^2.1",
        "maennchen/zipstream-php": "^2.1.0",
        "illuminate/support": "^8.49",
        "stof/doctrine-extensions-bundle": "^1.5",
        "symfony/psr-http-message-bridge" : "^2.0",
        "nyholm/psr7": "^1.4",
        "embed/embed": "^4.2"
